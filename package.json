{
  "name": "studio.thearcades.me",
  "version": "1.0.0",
  "description": "The Arcades Studio - A blog and portfolio site",
  "main": "index.js",
  "scripts": {
    "build": "npx @11ty/eleventy && npx tailwindcss -i ./src/css/style.css -o ./_site/css/style.css --minify",
    "dev": "npx @11ty/eleventy --serve & npx tailwindcss -i ./src/css/style.css -o ./_site/css/style.css --watch",
