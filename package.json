{
  "name": "studio.thearcades.me",
  "version": "1.0.0",
  "description": "The Arcades Studio - A blog and portfolio site",
  "main": "index.js",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "build": "npx @11ty/eleventy && npx tailwindcss -i ./src/css/style.css -o ./_site/css/style.css --minify",
    "dev:eleventy": "npx @11ty/eleventy --serve",
    "dev:tailwind": "npx tailwindcss -i ./src/css/style.css -o ./_site/css/style.css --watch",
    "dev": "npm run build && (npm run dev:eleventy & npm run dev:tailwind)",
    "start": "npm run dev"
  },
  "keywords": [
    "eleventy",
    "blog",
    "tailwindcss"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@tailwindcss/typography": "^0.5.16",
    "@vercel/analytics": "^1.5.0",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.21",
    "postcss-load-config": "^4.0.2",
    "tailwindcss": "^3.3.1",
    "yaml": "^2.7.0"
  }
}
